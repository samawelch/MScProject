issn = {03756742},
journal = {Journal of Geochemical Exploration},
keywords = {Diffuse contamination,Drainage,Flux,Metal mine,Point source,Tamar},
month = {feb},
number = {2-3},
pages = {116--124},
pmid = {23858},
publisher = {Elsevier},
title = {{Contaminant fluxes from point and diffuse sources from abandoned mines in the River Tamar catchment, UK}},
