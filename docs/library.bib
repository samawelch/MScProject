title = {{Medicines, shaken and stirred: a critical review on the ecotoxicology of pharmaceutical mixtures}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25405972 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4213595 http://rstb.royalsocietypublishing.org/content/369/1656/20130585.abstract?etoc},
