journal = {Environmental Toxicology and Chemistry},
keywords = {pesticides},
number = {1},
pages = {84--98},
title = {{Toxicity of Pesticides to aquatic microrganisms: a review}},
url = {https://setac.onlinelibrary.wiley.com/doi/pdf/10.1002/etc.5620200108},
volume = {20},
