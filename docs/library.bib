file = {:C$\backslash$:/Users/Sam Welch/Google Drive/ICL Ecological Applications/Project/Lab group resources/Papers/Microbes/Azarbad{\_}et{\_}al{\_}2016.pdf:pdf},
isbn = {1422-0067},
issn = {14220067},
journal = {International Journal of Molecular Sciences},
keywords = {Metal pollution,Microbial communities,Microbial stability,Resilience,Resistance,Secondary stressors},
number = {6},
pages = {1--20},
pmid = {27314330},
title = {{Resilience of soil microbial communities to metals and additional stressors: DNA-based approaches for assessing “stress-on-stress” responses}},
volume = {17},
year = {2016}
}
