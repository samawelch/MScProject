title = {{Dilute Concentrations of a Psychiatric Drug Alter Behavior of Fish from Natural Populations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23413353 http://www.sciencemag.org/cgi/doi/10.1126/science.1226850},
volume = {339},
year = {2013}
}
@article{Vinebrooke2004,
abstract = {Ecosystem resistance to a single stressor relies on tolerant species that can compensate for sensitive competitors and maintain ecosystem processes, such as primary production. We hypothesize that resistance to additional stressors depends increasingly on species tolerances being positively correlated (i.e. positive species co- tolerance). Initial exposure to a stressor combined with positive species co-tolerance should reduce the impacts of other stressors, which we term stress-induced community tolerance. In contrast, negative species co-tolerance is expected to result in additional stressors having pronounced additive or synergistic impacts on biologically impoverished functional groups, which we term stress-induced community sensitivity. Therefore, the sign and strength of the correlation between species sensitivities to multiple stressors must be considered when predicting the impacts of global change on ecosystem functioning as mediated by changes in biodiversity.},
author = {Vinebrooke, Rolf D and Cottingham, Kathryn L and Norberg, Jon and Scheffer, Marten and Dodson, Stanley I and Maberly, Stephen C and Sommer, Ulrich and Vinebrooke, Rolf D and Cottingham, Kathryn L and Norberg, Jon and Scheffer, Marten and Dodson, Stanley I and Maberly, Stephen C and Sommer, Ulrich},
doi = {10.1111/j.0030-1299.2004.13255.x},
file = {:C$\backslash$:/Users/Sam Welch/Google Drive/ICL Ecological Applications/Project/Papers/Impacts of multiple stressors on biodiversity and ecosystem functioning the role of species co‐tolerance (2004).pdf:pdf},
isbn = {0030-1299},
issn = {00301299},
journal = {Nordic Society Oikos},
number = {3},
pages = {451--457},
pmid = {8725094},
title = {{Impacts of Multiple Stressors on Biodiversity and Ecosystem Functioning : The Role of Species Co-Tolerance Published by : Wiley on behalf of Nordic Society Oikos Stable URL : http://www.jstor.org/stable/3547806 Accessed : 27-03-2016 0}},
volume = {104},
year = {2004}
}
