issn = {1461023X},
journal = {Ecology Letters},
keywords = {2018,Antagonism,antagonism,detritivores,dynamics,ecology letters,ecosystem services,energy budgets,individual-based model,individual‐based model,population,population dynamics,stressor interactions,synergism},
month = {apr},
number = {4},
pages = {568--577},
publisher = {Wiley/Blackwell (10.1111)},
title = {{When things don't add up: quantifying impacts of multiple stressors from individual metabolism to ecosystem processing}},
