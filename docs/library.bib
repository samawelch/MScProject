arxivId = {arXiv:1011.1669v3},
author = {Daughton, Christian G and Ternes, Thomas A},
doi = {10.1021/bk-2001-0791},
eprint = {arXiv:1011.1669v3},
file = {:C$\backslash$:/Users/Sam Welch/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Daughton, Ternes - 1999 - Pharmaceuticals and personal care products in the environment agents of subtle change.pdf:pdf},
