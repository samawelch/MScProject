pages = {864--877},
publisher = {Wiley-Blackwell},
title = {{Synergism and antagonism among multiple stressors}},
url = {http://doi.wiley.com/10.4319/lo.1999.44.3{\_}part{\_}2.0864 https://aslopubs.onlinelibrary.wiley.com/doi/pdf/10.4319/lo.1999.44.3{\_}part{\_}2.0864 http://www.avto.aslo.info/lo/toc/vol{\_}44/issue{\_}3{\_}part{\_}2/0864.pdf},
